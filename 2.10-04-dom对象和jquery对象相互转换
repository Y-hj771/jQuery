        //直接获取视频就是jquery对象
        // $('video');
        
        //我们是用原生js获取对象dom
        var myVideo =document.querySelector('video');
        //$(myVideo).play();jquery没有play这个方法

        //jquery对象转换为dom对象
        // myVideo.play();
        $(myVideo)[0].play();
        // $(myVideo).get(0).play();
    </script>
